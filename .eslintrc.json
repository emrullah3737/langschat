{
    "extends": "airbnb-base",
    "plugins": [
        "import"
    ],
    "rules": {
        "class-methods-use-this": 0,
        "no-console": 0,
        "no-unused-vars": 0,
        "no-underscore-dangle": 0,
        "global-require": 0,
        "consistent-return": 0,
        "import/no-dynamic-require": 0,
        "no-param-reassign": 0,
        "func-names": 0
    }
}